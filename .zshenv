# .zshenv
# this file is executed for *all* sessions.
# for code that is specific to interactive sessions, see .zshrc

export CABALDIR=~/.cabal
export PATH=~/bin:$CABALDIR/bin:/var/lib/gems/1.8/bin:/usr/local/sbin:/usr/local/bin:/opt/local/sbin:/opt/local/bin:/opt/jruby/bin:$PATH
export MANPATH=/opt/local/share/man:$CABALDIR/share/man:$MANPATH
